<% delivery = poll_feed_delivery %>
<% poll = delivery.feed_item.originator %>
<div id='feed_delivery_<%= delivery.id %>'>
	<div><%= avatar poll.poster %></div>
	<div><%= profile_link poll.poster %>发布了投票<%= poll_link poll %></div>
	<div><%= yield %></div> 
	<div><%= ftime delivery.created_at %></div>
</div>
