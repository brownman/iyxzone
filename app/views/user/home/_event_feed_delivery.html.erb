<% delivery = event_feed_delivery %>
<% event = delivery.feed_item.originator %>

<% render :layout => 'feed_delivery', :locals => {:delivery => delivery, :poster => event.poster} do %>
	    <% if delivery.is_deleteable_by? current_user %><%= link_to_remote "", :url => feed_delivery_url(delivery), :method => :delete, :html => {:class => "icon-active"} %><% end %> 
	    <%= profile_link event.poster %> 组织了活动 <%= event_link event, :class => "shine" %>
	    <p class='gray jl-con'><strong>游戏分类：<%= event.game.name %></strong></p>
	    <p class="gray"><%= ftime delivery.created_at %></p>
<% end %>
