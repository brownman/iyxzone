<% delivery = guild_feed_delivery %>
<% guild = delivery.feed_item.originator %>
<div class="feed-list-box">
<div class='feed-list' id='feed_delivery_<%= delivery.id %>'>
	<div class="img left">
		<div class="imgbox01"><%= avatar guild.president %></div>
	</div>
  <div class="jl-list-con jl-cutline"><%= profile_link guild.president %> 创建了工会 <%= guild_link guild, :class => "shine" %> 
	<% if delivery.is_deleteable_by? current_user %><%= link_to_remote "", :url => feed_delivery_url(delivery), :method => :delete, :html => {:class => "icon-active"} %><% end %> 
	<p class='gray jl-con'><strong>游戏分类：<%= guild.game.name %></strong></p>
	<p class="gray"><%= ftime delivery.created_at %> <%= link_to_function "评论", "Iyxzone.Comment.showForm('guild', #{guild.id}, null);", :class => "gray"%></p>
	<%= render :partial => "user/comments/folded_comment_box", :locals => {:commentable => guild , :recipient => guild.poster} %>
	</div>
</div>
</div>
