<% delivery = guild_feed_delivery %>
<% guild = delivery.feed_item.originator %>
<div class="feed-list-box">
<div class='feed-list' id='feed_delivery_<%= delivery.id %>'>
	<div class="img left">
		<div class="imgbox01"><%= avatar event.poster %></div>
	</div>
  <div class="jl-list-con jl-cutline"><%= profile_link guild.president %> 创建了工会 <%= guild_link guild, :class => "shine" %> 
	<% if delivery.is_deleteable_by? current_user %><%= link_to_remote "", :url => feed_delivery_url(delivery), :method => :delete, :class => "icon-active" %><% end %> 
	<p class="gray"><%= ftime delivery.created_at %> <%= link_to_function "回复", "Iyxzone.Comment.showForm('guild', #{guild.id}, null);", :class => "gray"%></p>
  <% if with_comments %>
	  <%= render :partial => "user/comments/folded_comment_box", :locals => {:commentable => guild , :recipient => guild.poster} %>
  <% end %>
	</div>
</div>
</div>
