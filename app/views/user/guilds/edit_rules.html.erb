<% content_for :javascript_include do %>
  <%= javascript_include_tag 'guild' %>
<% end %>

<% content_for :html_title, "#{@guild.name}的规则管理 - 17Gaming.com" %>
<% content_for :html_description, "#{@guild.name}的规则管理 - 17Gaming.com (一起游戏网)愿做网络游戏玩家的精神家园。" %>
<% content_for :html_keywords, "#{@guild.name}, 游戏活动, 17Gaming, 一起游戏, 一起游戏网, 网络游戏玩家, 游戏社区, 社交网站, 网络游戏社区" %>

<div id="canvas" class="round_r_t"><div class="round_l_t"><div class="round_r_b"><div class="round_l_b">
  <div class="round_m"><div class="round_body">
    <div class="friend-head space s_clear">
      <span class="left"><%= album_cover_image @guild.album, :class => "imgbox01", :width => 50, :height => 50 %></span>
      <p class="left">
        <strong>规则管理</strong><br/>
        <%= guild_link @guild %>的主页
      </p>
    </div>
    <div class="box02 canvas_int space">
      <div id='attendance_rule_frame' class='archives-box'>
        <%= render :partial => 'user/guilds/rules/attendance_rules', :locals => {:guild => @guild} %>
      </div>
      <div id='boss_frame' class='archives-box'>
        <%= render :partial => 'user/guilds/bosses/bosses', :locals => {:guild => @guild}%>
      </div>
      <div id='gear_frame' class='archives-box'>
        <%= render :partial => 'user/guilds/gears/gears', :locals => {:guild => @guild} %>
      </div>
      <div id='basic_rule_frame' class='archives-box'>
        <%= render :partial => 'user/guilds/rules/basic_rules', :locals => {:guild => @guild} %>
      </div>
    </div>
  </div></div>
</div></div></div></div>
