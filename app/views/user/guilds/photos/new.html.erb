<% content_for :html_title, "#{@guild.name}的相片上传 - 17Gaming.com" %>
<% content_for :html_description, "#{@guild.name}的相片上传 - 17Gaming.com (一起游戏网)愿做网络游戏玩家的精神家园。" %>
<% content_for :html_keywords, "#{@guild.name}, 游戏活动, 17Gaming, 一起游戏, 一起游戏网, 网络游戏玩家, 游戏社区, 社交网站, 网络游戏社区" %>

<div id="canvas" class="round_r_t"><div class="round_l_t"><div class="round_r_b"><div class="round_l_b">
  <div class="round_m"><div class="round_body">
    <div class="friend-head space s_clear">
      <span class="left"><%= album_cover_image @guild.album, :class => "imgbox01", :width => 50, :height => 50 %></span>
      <p class="left">
        <strong>上传照片</strong><br/>
        <%= guild_link @guild, :class => 'red' %>的主页
      </p>
    </div>
    <div class="box02 canvas_int">
      <div class="tab tab01">
        <div class="right">
          <% if @membership and (@membership.is_veteran? and @membership.is_president?) %>
          <%= link_to "上传照片", new_guild_photo_url(:album_id => @album.id) %>
          <% end %>
        </div>
        <ul>
          <li><span><%= link_to "热门照片", hot_personal_photos_url(:uid => current_user.id) %></span></li>
          <li><span><%= link_to "我的相册", personal_albums_url(:uid => current_user.id) %></span></li>
          <li><span><%= link_to '好友相册', friends_personal_albums_url(:uid => current_user.id) %></span></li>
          <li><span><%= link_to "圈了我的照片", relative_personal_photos_url(:uid => current_user.id) %></span></li>
        </ul>
      </div>
      <div class="box02 appcontent02">
        <div class='path-box'>
          <label>公会"<%= guild_link @guild %>"</label>
          <span>上传照片</span>
        </div>
        <%= swfupload_head %>
        <div class='photo-upload-box'>
          <%= swfupload :controller => 'user/guilds/photos', :action => 'create', :album_id => @album.id %>
        </div>
      </div>
    </div>
  </div></div>
</div></div></div></div>
