<% facebox_tag '修改截至时间' do %>
  <div id='errors'></div>
  <% rows_form_for :poll, :html => {:onsubmit=> "Iyxzone.Poll.updatePoll($(edit_poll_deadline_btn), this, #{@poll.id});return false"} do |f| %>
    <div class='formcontent'>
      <%= f.calendar_date_select :deadline, :label => '截至时间' %>
    </div>
    <%= f.z_submit :submit => {:text => '完成', :id => 'edit_poll_deadline_btn'}, :cancel => {:text => '取消', :onclick => 'Iyxzone.Facebox.close();'} %>
  <% end %>
<% end %>
