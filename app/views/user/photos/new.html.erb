<% content_for :html_title, "#{current_user.login} - 上传照片 - 17Gaming.com" %>
<% content_for :html_description, "#{current_user.login} - 上传照片 - 17Gaming.com (一起游戏网)愿做网络游戏玩家的精神家园。" %>
<% content_for :html_keywords, "#{current_user.login}, 游戏活动, 17Gaming, 一起游戏, 一起游戏网, 网络游戏玩家, 游戏社区, 社交网站, 网络游戏社区" %>

<% canvas_tag do %>
  <h1 class="app-photo">照片</h1>
  <div class="box02 canvas_int space">
    <%= render :partial => 'user/albums/tab', :locals => {:user => current_user, :hover => 1} %>
    <div class="box02 appcontent02">
      <div class='path-box'>
        <label><%= album_link @album %></label>
        <span>上传照片</span>
      </div>
      <%= swfupload_head %>
      <div class='photo-upload-box'>
        <%= swfupload :controller => 'user/photos', :action => 'create', :album_id => @album.id %>
      </div>
    </div>
  </div>
<% end %>
