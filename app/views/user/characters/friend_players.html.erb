<div id='game_friend_players'>
<p class="z-h s_clear">
  <strong class="left">玩 <%=@game.name-%> 的好友</strong>
  <%= link_to_function '', "Iyxzone.Facebox.close();", :class => "icon2-close right" %>
</p>

<div class='z-con'>
<div class='photolist-box'>
  <% unless @users.blank? %>
  <ul class="collection s_clear">
    <% @users.each do |u| %>
      <li>
        <div class="middle"><div class="middle-center"><div class="center"><a href="#"><%= avatar u, :class => 'imgbox01' %></a></div></div></div>
        <p><%= profile_link u %></p>
      </li>
    <% end %>
  </ul>
  <% else %>
    没有好友玩这个游戏
  <% end %>
</div>
</div>

<div class="buttons s_clear">
  <table class="center" cellpadding="0"><tr><td>
    <%= remote_will_paginate @users, :remote => @remote %>
  </td></tr></table>
</div>
</div>
