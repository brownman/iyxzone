          <div class="round06 round_r_t popMemberEx space">
            <div class="round_l_t">
              <div class="round_r_b">
                <div class="round_l_b">
                  <div class="round_m">
                    <div class="round_body">
                      <h2 class="title">人气排行榜</h2>
                      <ul class="list">
                        <% users.each do |user| %>
                        <li>
                          <div class="m-card">
                            <div class="m-avatar"><%= avatar user, :class => "imgbox01" %></div>
                            <div class="m-con">
                              <h4><%= profile_link user, :class => "member-name" %></h4>
                              <div><%= "关注人数：#{user.attentions_count}" %></div>
                              <% if user != current_user and !user.has_friend?(current_user) %>
                                <%= facebox_link '<span class="icon-friend02"></span>加为好友', new_friend_request_url(:friend_id => user.id), :class => "red" %>
                              <% end %>
                            </div>
                          </div>
                        </li>
                        <% end %>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>

