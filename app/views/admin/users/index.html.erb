<div id="canvas" class="round_r_t"><div class="round_l_t"><div class="round_r_b"><div class="round_l_b">
  <div class="round_m"><div class="round_body">
    <h1 class="app-log">用户</h1>
    <div class="box02 canvas_int">
      <div class="tab tab01">
        <div class='right'><%= text_field_tag '', ''%></div>
        <ul>
          <li class='hover'><span><%= link_to "已经激活的", admin_users_url %></span></li>
          <li><span><%= link_to "等待激活的", pending_admin_users_url %></span></li>
        </ul>
      </div>
      <div class="appcontent05">
        <div class="myblog">
          <h4 class="gray">共<%= User.count %>个用户</h4>
          <div class="myblog-table">
            <table cellpadding="0">
            <% @users.each do |user| %>
              <tr id='user_<%= user.id %>'>
                <td><div><%= profile_link user %></div></td>
                <td><%= user.email %></td>
                <% if @user == current_user %>
                  <td><%= link_to '', edit_blog_url(blog_item), :class => "icon-active icon-active-edit" %></td>
                  <td><%= link_to_remote '', blog_url(blog_item), :class => "icon-active", :method => :delete %></td>
                <% end %>
              </tr>
            <% end %>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div></div>
</div></div></div></div>

