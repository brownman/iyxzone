<div id="canvas" class="round_r_t"><div class="round_l_t"><div class="round_r_b"><div class="round_l_b">
  <div class="round_m"><div class="round_body">
    <h1 class="app-log">投票</h1>
    <div class="box02 canvas_int space">
      <div class="tab tab01">
        <ul>
          <li <% if params[:type].to_i == 0 %>class="hover"<% end %>><span><%= link_to "待审核投票", admin_polls_url(:type => 0) %></span></li>
          <li <% if params[:type].to_i == 1 %>class="hover"<% end %>><span><%= link_to "通过投票", admin_polls_url(:type => 1) %></span></li>
          <li <% if params[:type].to_i == 2 %>class="hover"<% end %>><span><%= link_to "屏蔽投票", admin_polls_url(:type => 2) %></span></li>
        </ul>
      </div>
      <div class="vote-box">
        <div class='gray'>共<%= @polls.total_entries %>篇</div>
        <table cellspacing=0 class='vote-table'>
        <%= render :partial => 'poll_item', :collection => @polls %>
        </table>
        <%= will_paginate @polls %>
      </div>
    </div>
  </div></div>
</div></div></div></div>
