  [36mtest[m [35m"[m[31m请求参加活动后，活动的计数器加1，活动组织者的请求计数器加1[m[35m"[m [33mdo[m
    [36m@event1[m.participations.create([31m:participant_id[m => [36m@user2[m.id, [31m:status[m => [31m1[m)
    reload
    [36massert_equal[m [36m@event1[m.poster.event_requests_count, [31m1[m
    [36massert_equal[m [36m@event1[m.requestors_count, [31m1[m
    [36m@event1[m.participations.create([31m:participant_id[m => [36m@user3[m.id, [31m:status[m => [31m2[m)
    reload
    [36massert_equal[m [36m@event1[m.poster.event_requests_count, [31m2[m
    [36massert_equal[m [36m@event1[m.requestors_count, [31m2[m
  [33mend[m

  [36mtest[m [35m"[m[31m活动请求被活动组织者同意[m[35m"[m [33mdo[m
    [36m@p1[m = [36m@event1[m.participations.create([31m:participant_id[m => [36m@user2[m.id, [31m:status[m => [31m1[m)
    [36m@p2[m = [36m@event1[m.participations.create([31m:participant_id[m => [36m@user3[m.id, [31m:status[m => [31m2[m)
    [36m@p1[m.reload [33mand[m [36m@p1[m.accept
    reload
    [36massert_equal[m [36m@p1[m.status, [31m3[m
    [36massert_equal[m [36m@event1[m.requestors_count, [31m1[m
    [36massert_equal[m [36m@event1[m.poster.event_requests_count, [31m1[m
    [36massert_equal[m [36m@user2[m.upcoming_events_count, [31m1[m
    [36m@p2[m.reload [33mand[m [36m@p2[m.accept
    reload
    [36massert_equal[m [36m@p2[m.status, [31m4[m
    [36massert_equal[m [36m@event1[m.requestors_count, [31m0[m
    [36massert_equal[m [36m@event1[m.poster.event_requests_count, [31m0[m
    [36massert_equal[m [36m@user2[m.upcoming_events_count, [31m1[m
  [33mend[m

  [36mtest[m [35m"[m[31m活动请求被活动组织者拒绝[m[35m"[m [33mdo[m
    [36m@p1[m = [36m@event1[m.participations.create([31m:participant_id[m => [36m@user2[m.id, [31m:status[m => [31m1[m)
    [36m@p2[m = [36m@event1[m.participations.create([31m:participant_id[m => [36m@user3[m.id, [31m:status[m => [31m2[m)
    [36m@p1[m.reload [33mand[m [36m@p1[m.destroy [34m# @p1.reload 清空 association_cache, 当有2个或者2个以上改变操作的时候，需要特别注意association_cache是否会破坏你的代码[m
    reload
    [36massert_equal[m [36m@event1[m.requestors_count, [31m1[m
    [36massert_equal[m [36m@event1[m.poster.event_requests_count, [31m1[m
